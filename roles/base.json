    "authorization": {
      "sudo": {
        "groups": [
          "admin",
          "wheel",
          "sysadmin"
        ]
      }
    }
  },
  "override_attributes": {},
  "chef_type": "role",
  "run_list": [
    "recipe[users::sysadmins]",
    "recipe[sudo]",
    "recipe[build-essential]",
    "recipe[git]",
    "recipe[vim]",
    "recipe[mosh]",
    "recipe[tmux]",
    "recipe[soupstraw::user_configs]"
  ],
  "env_run_lists": {}
}
